@using MtdKey.OrderMaker.Components
@model MTDImgViewerModel

@using Microsoft.AspNetCore.Mvc.Localization
@inject IHtmlLocalizer<SharedResource> Localizer
@{
    //image/svg+xml

    string imgBody = "data:image/png;base64,";
    if (Model.ImgArray != null)
    {
        string base64 = Convert.ToBase64String(Model.ImgArray);
        imgBody = string.Format("data:{0};base64,{1}", Model.ImgType, base64);
    }

    string width = @Model.Width != -1 ? $" width:{@Model.Width}px; " : string.Empty;
    string maxWidth = @Model.MaxWidth != -1 ? $" max-width:{@Model.MaxWidth}px; " : string.Empty;
    string height = @Model.Height != -1 ? $" height:{@Model.Height}px; " : string.Empty;
    string maxHeight = @Model.MaxHeight != -1 ? $" max-height:{@Model.MaxHeight}px; " : string.Empty;   

    string style = string.Concat(width, maxWidth, height, maxHeight);

}

<img src="@imgBody" @if (@style.Length > 0) { <text> style="@style" </text> } />