@model Core.DocModel

<div class="mtd-store-edit-rows">
    @foreach (var field in Model.Fields)
    {
        <div class="mtd-store-edit-item">
            @await Component.InvokeAsync("StoreStackEditor", new { docPart = Model.Parts.FirstOrDefault(), docField = field })
        </div>
    }
</div>
